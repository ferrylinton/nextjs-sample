{
  "openapi": "3.0.0",
  "info": {
    "title": "Next Swagger API Example",
    "version": "1.0"
  },
  "paths": {
    "/api/hello": {
      "get": {
        "description": "Returns the hello world",
        "responses": {
          "200": {
            "description": "hello world",
            "schema": {
              "$ref": "#/components/schema/Hello"
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Hello": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "The people name"
          }
        },
        "example": {
          "name": "Jhon Doe"
        }
      }
    }
  },
  "tags": []
}